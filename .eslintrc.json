{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "airbnb/hooks", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],

    "rules": {
      "react/jsx-no-constructed-context-values":"off",
      "np-underscore-dangle":"off",
      "linebreak-style":"off",
      "double-quoted": "off",
      "quotes":"off"
    }

}
